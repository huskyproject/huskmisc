# Microsoft Visual C++ Makefile
#

DEBUG=0
CC=cl
EXENAMEFLAG=-Fe
SRC_DIR=..\src\

CFLAGS=-nologo -MD -Zp1 -Ob2 -I..\h -I..\.. -c
LFLAGS=
CDEFS =-D__NT__ -D_CONSOLE -D_MAKE_DLL

OBJ=.obj
EXE=.exe
RM=del

LIBS=..\..\fidoconf\fconfmvc.lib ..\..\smapi\smapimvc.lib USER32.LIB msvcrt.lib Kernel32.lib /link /nodefaultlib /opt:nowin98

!include makefile.inc

default: all

{..\tools}.c$(OBJ):
	$(CC) $(CFLAGS) $(CDEFS) $<

all: commonprogs

clean: commonclean

distclean: commondistclean
